@model List<Bil372_Odev1_Grup6.Models.BRAND_ORGS>

<body>
    <div>
        <div class="col-lg-9">
            <div class="table-responsive table--no-card m-b-30">
                <table class="table table-borderless table-striped table-earning">
                    <thead>
                        <tr>
                            <th>Organizasyon Kod</th>
                            <th>Marka Barkod</th>
                            <th>Miktar</th>
                            <th>Birim</th>
                            <th>Girdi</th>
                            <th>Çıktı</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)

                        {
                              if (Session["userID"].ToString().Equals(@item.ORG_ID.ToString()))
                            {
                                <tr>
                                    <td>@item.ORG_ID</td>
                                    <td>@item.BRAND_BARCODE</td>
                                    <td>@item.QUANTITY</td>
                                    <td>@item.UNIT</td>
                                    <td>@item.INNN</td>
                                    <td>@item.OUTTTT</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <div class="card">
        <div class="card-header">
            <strong>İç Transfer</strong>
        </div>
        <div class="card-body card-block">
            <form action="" method="post" class="">
                <div class="form-group">
                    <input type="hidden" id="sourceid" name="sourceid" value="@Session["userID"]">
                </div>
                <div class="form-group">
                    <label for="nf-email" class=" form-control-label">Marka Barkod</label>
                    <input id="bbarcode" name="bbarcode" placeholder="" class="form-control">
                </div>
                <div class="form-group">
                    <label for="nf-email" class=" form-control-label">Miktar</label>
                    <input id="quantity" name="quantity" placeholder="" class="form-control">
                </div>

                <input type="submit" class="btn btn-primary btn-sm" value="Ekle">
            </form>
        </div>
        <div class="card-footer">

        </div>
    </div>
</body>

